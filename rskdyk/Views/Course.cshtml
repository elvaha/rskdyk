@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}

<div class="fold">
    <div class="fold__content">
        <header class="fold__header">
            <h1 class="fold__title">@Umbraco.Field("title")</h1>
        </header>
    </div>
</div>
<section class="information">
	<p class="information__date">@CurrentPage.startingDate.ToString("yyyy/mm/dd")</p>
    <div class="information__content">@Umbraco.Field("description")</div>
</section>